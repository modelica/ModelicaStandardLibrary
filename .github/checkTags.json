{
    "problemMatcher": [
        {
            "owner": "checkTags",
            "pattern": [
                {
                    "regexp": "^File\\s\"(?:\\.\\/)?([^\"]*)\":\\sline\\s(\\d+)\\s-\\s(Error|Warning|Info):\\s(.*)$",
                    "file": 1,
                    "line": 2,
                    "severity": 3,
                    "message": 4
                }
            ]
        }
    ]
}
